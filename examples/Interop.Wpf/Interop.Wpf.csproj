<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <LangVersion>preview</LangVersion>
    <!-- Important!!! Must set UICulture because markup compile pass 2 doesn't support source generators!!! (yet??) -->
    <UICulture>en-US</UICulture>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\StandardUI.Base\StandardUI.Base.csproj" />
    <ProjectReference Include="..\..\src\StandardUI.Wpf.Generator\StandardUI.Wpf.Generator.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    <ProjectReference Include="..\..\src\StandardUI.Wpf\StandardUI.Wpf.csproj" />
  </ItemGroup>

</Project>
